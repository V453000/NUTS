		purchase_cargo_capacity:   30;
		loading_speed:		6;//switch_loadspeed_global;
		cost_factor:		10;//switch_cost_global;
		weight:				15;
		}
		}
if (param_wagons == 0) {
	item(FEAT_TRAINS, item_railflatbed3, 412) {
