        vehicle_life:                 255;
        reliability_decay:            5;
				cargo_capacity:				 			  80; // shows as 80 passengers in game
        refittable_cargo_classes:     bitmask(CC_PASSENGERS, CC_MAIL, CC_ARMOURED);
        non_refittable_cargo_classes: bitmask();
        default_cargo_type:			      PASS;
		 		cargo_allow_refit:		        [YETI, YETY];
        loading_speed:                5;
        cost_factor:                  10;
        running_cost_factor:          100;
        sprite_id:                    SPRITE_ID_NEW_TRAIN;
        speed:                        100 km/h;
        misc_flags:                   bitmask(TRAIN_FLAG_FLIP, TRAIN_FLAG_MU, TRAIN_FLAG_2CC);
        refit_cost:                   0;
        track_type:                   RAIL;
        ai_special_flag:              AI_FLAG_CARGO;
        power:                        2300 hp;
        running_cost_base:            RUNNING_COST_ELECTRIC;
        dual_headed:                  1;
        weight:                       80 ton;
        ai_engine_rank:               0;
        engine_class:                 ENGINE_CLASS_ELECTRIC;
        extra_power_per_wagon:        0 kW;
        tractive_effort_coefficient:  0.2;
        air_drag_coefficient:         0;
        length: 8;
        visual_effect_and_powered:    visual_effect_and_powered(VISUAL_EFFECT_DEFAULT, 2, DISABLE_WAGON_POWER);
        extra_weight_per_wagon:       0 ton;
        bitmask_vehicle_info:         0;
	}
	graphics {
				default: switch_railcom5_front;//switch_railcom5;
		cargo_subtype_text: railcom5_subtype_text;
		purchase: spriteset_purchaseMenu_raillocal5;
		can_attach_wagon: switch_railcom5_attach;
		additional_text: string(info_rail_local_5);
		speed: 							raillocal5_wetrail_speed_switch; //maybe for wagons later, not now
		power: 							raillocal5_wetrail_power_switch; //engine only
		weight: 						20; //engine only
		tractive_effort_coefficient:	raillocal5_wetrail_te_switch; //engine only
		loading_speed:					switch_pax_loadspeed; //express engines AND all wagons
		cargo_capacity:					railpax40_capacity_switch; //express engines AND all wagons
		//purchase_cargo_capacity:		80;
		cost_factor:						9; //everything
		running_cost_factor:				100; //only engines
		purchase_tractive_effort_coefficient: 61;
		purchase_power: 850;
		purchase_speed: 110;
	}

}
