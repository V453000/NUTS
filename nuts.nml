	param 0 {
	param_wagons {
		type: int;
		name: string(STR_PARAM_WAGONS);
		desc: string(STR_PARAM_PARAM_WAGONS_DESC);
		min_value: 0;
		max_value: 1;
		def_value: 0;
			names: {
 				0: string(STR_PARAM_WAGONS_REDUCED);
 				1: string(STR_PARAM_WAGONS_ALL);
				};
			}
		}
